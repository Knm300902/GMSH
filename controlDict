FoamFile
{
    format      ascii;
    class       dictionary;
    object      controlDict;
}

application     laplacianFoam; 

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         300;

deltaT          1;

writeControl    timeStep;

writeInterval   5;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

